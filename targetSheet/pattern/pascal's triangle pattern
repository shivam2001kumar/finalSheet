#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int icj;
    for(int i=0;i<n;i++)
    {
         icj=1;
        for(int j=0;j<=i;j++)
        {
            cout<<icj<<"\t";
            int icjp1=icj*(i-j)/(j+1);
            icj=icjp1;

        }
        // cout<<1;
        cout<<"\n";
    }
}

// ncr+1 = ncr*(n-r)/(r+1)

